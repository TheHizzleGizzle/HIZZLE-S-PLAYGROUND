{
  "name": "codecreature",
  "version": "1.0.0",
  "description": "A Pokemon-style creature collection game with coding abilities",
  "main": "dist/main/main.js",
  "scripts": {
    "build": "webpack --mode production",
    "dev": "webpack serve --mode development",
    "electron": "electron .",
    "electron:dev": "concurrently \"npm run dev\" \"wait-on http://localhost:8080 && electron .\"",
    "start": "npm run electron:dev"
  },
  "keywords": ["game", "coding", "pokemon", "education"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "phaser": "^3.70.0",
    "blockly": "^10.4.2",
    "monaco-editor": "^0.44.0",
    "@reduxjs/toolkit": "^2.0.1",
    "react-redux": "^9.0.4",
    "brython": "^3.12.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "typescript": "^5.3.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "html-webpack-plugin": "^5.5.3",
    "ts-loader": "^9.5.1",
    "css-loader": "^6.9.0",
    "style-loader": "^3.3.3",
    "electron": "^28.0.0",
    "concurrently": "^8.2.2",
    "wait-on": "^7.2.0",
    "copy-webpack-plugin": "^11.0.0"
  }
}
